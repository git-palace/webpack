{
  "name": "webpack-defaults",
  "version": "0.0.0",
  "description": "Defaults to be shared across webpack projects",
  "author": {
    "name": "Artem Sapegin",
    "url": "http://sapegin.me"
  },
  "homepage": "https://github.com/webpack-contrib/webpack-defaults",
  "repository": "webpack-contrib/webpack-defaults",
  "license": "MIT",
  "engines": {
    "node": ">=6"
  },
  "bin": "dist/index.js",
  "files": [
    "dist",
    "config.json",
    ".github"
  ],
  "scripts": {
    "sync": "./dist/index.js",
    "lint": "eslint --cache src test",
    "test": "jest",
    "prebuild": "npm run clean:dist",
    "build": "cross-env NODE_ENV=production babel -s true src -d dist --ignore 'src/**/*.test.js'",
    "clean:dist": "del-cli dist",
    "start": "npm run serve:dev src",
    "serve:dev": "nodemon $2 --exec babel-node",
    "security": "nsp check",
    "test:coverage": "jest --collectCoverageFrom='src/**/*.js' --coverage",
    "travis:test": "yarn run test",
    "travis:coverage": "yarn run test:coverage",
    "travis:lint": "yarn run lint && yarn run nsp"
  },
  "dependencies": {
    "chalk": "^1.1.3",
    "mrm-core": "^0.3.2"
  },
  "keywords": [
    "webpack"
  ],
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-jest": "^18.0.0",
    "babel-polyfill": "^6.22.0",
    "babel-preset-webpack": "^1.0.0",
    "cross-env": "^3.1.4",
    "del-cli": "^0.2.1",
    "eslint": "^3.14.1",
    "eslint-config-webpack": "^1.0.0",
    "eslint-plugin-import": "^2.2.0",
    "jest": "^18.1.0",
    "nodemon": "^1.11.0",
    "nsp": "^2.6.2"
  }
}
